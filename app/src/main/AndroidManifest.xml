<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shubhigupta.quizoidnew">

    <uses-permission
        android:name="android.permission.CALL_PHONE"
        android:protectionLevel="signature|privileged" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/unnamed"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginMain" />
        <activity android:name=".AppIntro" />
        <activity android:name=".QuestionsListPage" />
        <activity android:name=".Register_activity" />
        <activity android:name=".QuestionsSubCategories.SublistActivity" />
        <activity android:name=".LastActivity" />
        <activity android:name=".GameStartActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".HighScoreBoard" />
        <activity android:name=".questions_new" />
        <activity android:name=".HowToPlay"></activity>
    </application>

</manifest>